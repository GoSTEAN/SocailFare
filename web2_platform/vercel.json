{
  "version": 2,
  "builds": [
      {
          "src": "web2_platform/wsgi.py",
          "use": "@vercel/python"
      }
  ],
  "routes": [
      {
          "src": "/static/(.*)",
          "dest": "/static/$1"
      },
      {
          "src": "/(.*)",
          "dest": "web2_platform/wsgi.py"
      }
  ],
  "env": {
      "DJANGO_SETTINGS_MODULE": "web2_platform.settings",
      "PYTHONPATH": "."
  }
}